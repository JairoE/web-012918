<%= form_for(@student) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :date_of_birth %>
  <%= f.date_field :dob %>
  <%= f.submit %>
<% end %>

<%= form_tag '/posts' do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %>
  <%= submit_tag %>
<% end %>
